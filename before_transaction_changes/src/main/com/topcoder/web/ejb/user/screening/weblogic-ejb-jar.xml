<?xml version="1.0"?>

<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB//EN' 'http://www.bea.com/servers/wls510/dtd/weblogic-ejb-jar.dtd'>

<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>User</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
        </caching-descriptor>
        <jndi-name>screening:com.topcoder.web.ejb.user.UserHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>UserTermsOfUse</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
        </caching-descriptor>
        <jndi-name>screening:com.topcoder.web.ejb.user.UserTermsOfUseHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>UserSchool</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
        </caching-descriptor>
        <jndi-name>screening:com.topcoder.web.ejb.user.UserSchoolHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>Contact</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
            <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
        </caching-descriptor>
        <jndi-name>screening:com.topcoder.web.ejb.user.ContactHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>UserAddress</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
            <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
        </caching-descriptor>
        <jndi-name>screening:com.topcoder.web.ejb.user.UserAddressHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>UserNote</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
        </caching-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>datasource</res-ref-name>
                <jndi-name>SCREENING_OLTP</jndi-name>
            </resource-description>
            <resource-description>
                <res-ref-name>jts_datasource</res-ref-name>
                <jndi-name>JTS_SCREENING_OLTP</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>screening:com.topcoder.web.ejb.user.UserNoteHome</jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>

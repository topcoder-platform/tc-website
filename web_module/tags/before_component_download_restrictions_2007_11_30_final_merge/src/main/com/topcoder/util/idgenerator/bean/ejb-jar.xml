<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
    <display-name>IdGenerator</display-name>
    <enterprise-beans>
        <session>
            <ejb-name>IdGenEJB</ejb-name>
            <home>com.topcoder.util.idgenerator.bean.IdGenHome</home>
            <remote>com.topcoder.util.idgenerator.bean.IdGen</remote>
            <local-home>com.topcoder.util.idgenerator.bean.LocalIdGenHome</local-home>
            <local>com.topcoder.util.idgenerator.bean.LocalIdGen</local>
            <ejb-class>com.topcoder.util.idgenerator.bean.IdGenBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Bean</transaction-type>
            <env-entry>
                <env-entry-name>DBClassName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>com.topcoder.util.idgenerator.sql.SimpleDB</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>tableName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>sequence_object</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>userDefColumnName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>name</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>highValueColumnName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>current_value</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>maxHi</env-entry-name>
                <env-entry-type>java.lang.Long</env-entry-type>
                <env-entry-value>9999999999</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>maxLo</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>1</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>autoInit</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <resource-ref>
                <res-ref-name>jdbc/DefaultDS</res-ref-name>
                <res-type>javax.sql.DataSource</res-type>
                <res-auth>Container</res-auth>
            </resource-ref>
        </session>
    </enterprise-beans>
</ejb-jar>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.common.model.algo">

    <class name="Round" table="round" mutable="false">
        <id name="id" column="round_id" access="field" />
        <many-to-one name="contest" column="contest_id" class="Contest" access="field"/>

        <property name="name" column="name" access="field"/>
        <property name="shortName" column="short_name" access="field"/>
        <property name="status" column="status" access="field"/>
    </class>
</hibernate-mapping>
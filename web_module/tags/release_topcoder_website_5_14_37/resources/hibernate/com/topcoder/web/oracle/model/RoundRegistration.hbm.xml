<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.oracle.model" auto-import="false">

    <class name="RoundRegistration" table="oracle_oltp:round_registration">
        <composite-id name="id" class="RoundRegistration$Identifier">
            <key-many-to-one name="user" class="com.topcoder.web.common.model.User" column="user_id"/>
            <key-many-to-one name="round" class="Round" column="round_id"/>
        </composite-id>

        <many-to-one name="terms" column="terms_of_use_id" class="com.topcoder.web.common.model.TermsOfUse"
                     access="field" cascade="none"/>

    </class>

</hibernate-mapping>
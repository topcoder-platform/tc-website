                <java-type>java.lang.Character</java-type>
                <jdbc-type>CHAR</jdbc-type>
                <sql-type>CHAR(1)</sql-type>
            </mapping>
            <mapping>
                <java-type>java.lang.String</java-type>
                <jdbc-type>LONGVARCHAR</jdbc-type>
                <sql-type>VARCHAR(255,0)</sql-type>
            </mapping>

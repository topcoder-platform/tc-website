<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.common.model">

    <class name="CoderType" table="coder_type" mutable="false">
        <id name="id" column="coder_type_id" access="field"/>
        <property name="description" column="coder_type_desc" access="field"/>
        <property name="status" access="field"/>
    </class>

</hibernate-mapping>
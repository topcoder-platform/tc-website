<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.oracle.model" auto-import="false">
    <class name="Prediction" table="oracle_oltp:prediction">
        <id name="id" column="prediction_id">
            <generator class="com.topcoder.web.common.model.IdGenerator">
                <param name="sequence_name">ORACLE_PREDICTION_SEQ</param>
                <param name="return_class">java.lang.Integer</param>
            </generator>
        </id>
        <many-to-one name="user" column="user_id" class="com.topcoder.web.common.model.User"/>
        <many-to-one name="room" column="room_id" class="Room"/>
        <many-to-one name="candidate" column="candidate_id" class="Candidate"/>

        <property name="value"/>
        <property name="score"/>

    </class>

</hibernate-mapping>
<?xml version="1.0"?>

<!DOCTYPE ejb-jar PUBLIC
      "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
      "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
    <description>PACTS Services Jar</description>
    <display-name>PACTS Services Jar</display-name>
    <enterprise-beans>
        <session>
            <description>PACTS Services EJB</description>
            <display-name>PACTS Services EJB</display-name>
            <ejb-name>PactsServices</ejb-name>
            <home>com.topcoder.web.ejb.pacts.PactsServicesHome</home>
            <remote>com.topcoder.web.ejb.pacts.PactsServices</remote>
            <local>com.topcoder.web.ejb.pacts.PactsServicesLocal</local>
            <local-home>com.topcoder.web.ejb.pacts.PactsServicesLocalHome</local-home>
            <ejb-class>com.topcoder.web.ejb.pacts.PactsServicesBean</ejb-class>
            <transaction-type>Bean</transaction-type>
            <session-type>Stateless</session-type>
        </session>
        <session>
            <description>PACTS Client Services EJB</description>
            <display-name>PACTS Client Services EJB</display-name>
            <ejb-name>PactsClientServices</ejb-name>
            <home>com.topcoder.web.ejb.pacts.PactsClientServicesHome</home>
            <remote>com.topcoder.web.ejb.pacts.PactsClientServices</remote>
            <ejb-class>com.topcoder.web.ejb.pacts.PactsServicesBean</ejb-class>
            <transaction-type>Bean</transaction-type>
            <session-type>Stateless</session-type>
        </session>
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>PactsServices</ejb-name>
                <method-name>*</method-name>
            </method>
            <method>
                <ejb-name>PactsClientServices</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>



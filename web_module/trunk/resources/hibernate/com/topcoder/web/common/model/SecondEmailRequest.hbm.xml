<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.common.model">

	<class name="SecondEmailRequest" table="common_oltp:second_email_request">
		<id name="userId" column="user_id" access="field" />
		<property name="secondMail" column="email" access="field" />
		<property name="expiredAt" column="expired_at" access="field" />
		<property name="randomeKey" column="random_key" access="field" />
	</class>
</hibernate-mapping>
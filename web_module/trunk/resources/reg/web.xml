<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app
        version="2.4"
        xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <!--
        <distributable/>
    -->
    <servlet>
        <servlet-name>regservlet</servlet-name>
        <servlet-class>com.topcoder.web.reg.controller.RegServlet</servlet-class>
        <init-param>
            <param-name>module_key</param-name>
            <param-value>module</param-value>
        </init-param>
        <init-param>
            <param-name>default_processor</param-name>
            <param-value>Selection</param-value>
        </init-param>
        <init-param>
            <param-name>login_processor</param-name>
            <param-value>Login</param-value>
        </init-param>
        <init-param>
            <param-name>login_servlet</param-name>
            <param-value>/</param-value>
        </init-param>
        <init-param>
            <param-name>processor_path</param-name>
            <param-value>com.topcoder.web.reg.controller.request.</param-value>
        </init-param>
        <init-param>
            <param-name>error_page</param-name>
            <param-value>/error.jsp</param-value>
        </init-param>
        <!--        <load-on-startup>2</load-on-startup>-->
    </servlet>


<!--
    <filter>
        <filter-name>GZIPResponse</filter-name>
        <filter-class>com.topcoder.web.common.GZIPResponseFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>GZIPResponse</filter-name>
        <servlet-name>regservlet</servlet-name>
    </filter-mapping>
-->

    <servlet-mapping>
        <servlet-name>regservlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>60</session-timeout>
    </session-config>

    <error-page>
        <error-code>404</error-code>
        <location>/errorPage.jsp</location>
    </error-page>

    <!--    <listener>
        <listener-class>
            com.topcoder.web.common.SessionWatcher
        </listener-class>
    </listener>-->

</web-app>








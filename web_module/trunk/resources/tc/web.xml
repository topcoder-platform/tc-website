<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
 "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>
    <servlet>
        <servlet-name>tc</servlet-name>
        <display-name>tc</display-name>
        <description>The main controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.MainServlet</servlet-class>

        <!-- begin parameters required by BaseServlet -->
        <init-param>
            <param-name>module_key</param-name>
            <param-value>module</param-value>
            <description>the url parameter that defines which processor to execute</description>
        </init-param>
        <init-param>
            <param-name>default_processor</param-name>
            <param-value>Static</param-value>
            <description>the processor to execute if non is specified in the request</description>
        </init-param>
        <init-param>
            <param-name>login_processor</param-name>
            <param-value>Login</param-value>
            <description>the processor to execute when we need to direct the user to login</description>
        </init-param>
<!--
        <init-param>
            <param-name>login_servlet</param-name>
            <param-value>/tc</param-value>
            <description>the servlet path to use when we need to direct the user to login</description>
        </init-param>
-->
        <init-param>
            <param-name>processor_path</param-name>
            <param-value>com.topcoder.web.tc.controller.request.</param-value>
            <description>the path to the processors for this app</description>
        </init-param>
        <init-param>
            <param-name>error_page</param-name>
            <param-value>/errorPage.jsp</param-value>
            <description>where to forward for errors</description>
        </init-param>
        <!-- end  parameters required by BaseServlet -->

        <init-param>
            <param-name>static_prefix</param-name>
            <param-value>d</param-value>
            <description></description>
        </init-param>

        <!-- pages -->
        <init-param>
            <param-name>default_page</param-name>
            <param-value>/review_board/competition.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>survey_results</param-name>
            <param-value>/survey/results.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>survey_view</param-name>
            <param-value>/survey/view.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>survey_list</param-name>
            <param-value>/survey/list.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>survey_thanks</param-name>
            <param-value>/survey/thanks.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>problem_rating_questions</param-name>
            <param-value>/problem_rating/questions.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>crpf_report</param-name>
            <param-value>/crpf/report.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>problem_rating_results</param-name>
            <param-value>/problem_rating/results.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>problem_rating_question_info</param-name>
            <param-value>/problem_rating/question_info.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>highest_rating</param-name>
            <param-value>/statistics/highestRating.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>login</param-name>
            <param-value>/authentication/login.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>activate</param-name>
            <param-value>/authentication/activate.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>recover_password</param-name>
            <param-value>/authentication/recoverPassword.jsp</param-value>
            <description></description>
        </init-param>
        <init-param>
            <param-name>email_sent</param-name>
            <param-value>/authentication/emailSent.jsp</param-value>
            <description></description>
        </init-param>


        <!-- processor mappings -->
        <init-param>
            <param-name>SurveyResults</param-name>
            <param-value>survey.Results</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>SubmitSurvey</param-name>
            <param-value>survey.Submit</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>ViewSurvey</param-name>
            <param-value>survey.View</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>SurveyList</param-name>
            <param-value>survey.SurveyList</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>ProblemRatingQuestions</param-name>
            <param-value>problemRating.Questions</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>ProblemRatingResults</param-name>
            <param-value>problemRating.Results</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>SubmitRatings</param-name>
            <param-value>problemRating.Submit</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>CRPFReport</param-name>
            <param-value>crpf.Report</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>HighestRating</param-name>
            <param-value>statistics.HighestRating</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>Login</param-name>
            <param-value>authentication.Login</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>Logout</param-name>
            <param-value>authentication.Logout</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>PasswordEmail</param-name>
            <param-value>authentication.PasswordEmail</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>Activate</param-name>
            <param-value>authentication.Activate</param-value>
            <description></description>
        </init-param>


        <!-- request parameters -->
        <init-param>
            <param-name>problem_id</param-name>
            <param-value>pid</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>survey_id</param-name>
            <param-value>sid</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>first_name</param-name>
            <param-value>fn</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>last_name</param-name>
            <param-value>ln</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>email</param-name>
            <param-value>email</param-value>
            <description></description>
        </init-param>

        <init-param>
            <param-name>activation_code</param-name>
            <param-value>code</param-value>
            <description></description>
        </init-param>

    </servlet>

    <servlet>
        <servlet-name>legacyMain</servlet-name>
        <display-name>legacyMain</display-name>
        <description>The main controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.MainServlet</servlet-class>
    </servlet>

    <servlet>
      <servlet-name>stat</servlet-name>
      <servlet-class>com.topcoder.web.tc.controller.legacy.stat.servlet.StatisticsHttpServlet</servlet-class>
      <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>Registration</servlet-name>
        <display-name>Registration</display-name>
        <description>The registration controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.reg.servlet.Controller</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>jobposting</servlet-name>
        <display-name>jobposting</display-name>
        <description>The jobposting controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.jobposting.servlet.Controller</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Resume</servlet-name>
        <display-name>Resume</display-name>
        <description>The Resume controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.resume.servlet.Controller</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>graph</servlet-name>
        <display-name>graph</display-name>
        <description>The graph controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.GraphServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>javadocs</servlet-name>
        <display-name>javadocs</display-name>
        <description>The javadocs controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.JavaDocServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>report</servlet-name>
        <display-name>report</display-name>
        <description>The report controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.ReportServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PactsMemberServlet</servlet-name>
        <display-name>PactsMemberServlet</display-name>
        <description>The PactsMemberServlet controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.pacts.servlet.PactsMemberServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PactsInternalServlet</servlet-name>
        <display-name>PactsInternalServlet</display-name>
        <description>The PactsInternalServlet controller servlet</description>
        <servlet-class>com.topcoder.web.tc.controller.legacy.pacts.servlet.PactsInternalServlet</servlet-class>
    </servlet>

    <servlet-mapping>
      <servlet-name>PactsInternalServlet</servlet-name>
      <url-pattern>/PactsInternalServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>PactsMemberServlet</servlet-name>
      <url-pattern>/PactsMemberServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>report</servlet-name>
      <url-pattern>/report</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>javadocs</servlet-name>
      <url-pattern>/javadocs</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>graph</servlet-name>
      <url-pattern>/graph</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>Resume</servlet-name>
      <url-pattern>/Resume</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>stat</servlet-name>
      <url-pattern>/stat</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>legacyMain</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>tc</servlet-name>
        <url-pattern>/tc</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>jobposting</servlet-name>
        <url-pattern>/jobposting</url-pattern>
    </servlet-mapping>

</web-app>

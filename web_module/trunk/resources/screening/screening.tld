        <tagclass>com.topcoder.web.common.tag.ResultSetItemTag</tagclass>
        <bodycontent>empty</bodycontent>
        <info>
            Tag to get the value of a ResultSetContainer.ResultSetRow
        </info>
        <attribute>
            <name>row</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>format</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>ifNull</name>
            <required>false</required>

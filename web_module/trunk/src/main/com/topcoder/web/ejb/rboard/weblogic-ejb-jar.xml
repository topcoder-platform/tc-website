<?xml version="1.0"?>

<!DOCTYPE weblogic-ejb-jar PUBLIC
 '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB//EN'
 'http://www.beasys.com/j2ee/dtds/weblogic-ejb-jar.dtd'>

<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>RBoardApplication</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
            <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
        </caching-descriptor>
        <jndi-name>com.topcoder.web.ejb.rboard.RBoardApplicationHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>RBoardUser</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
            <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
        </caching-descriptor>
        <jndi-name>com.topcoder.web.ejb.rboard.RBoardUserHome</jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>

<?xml version="1.0"?>

<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB//EN' 'http://www.bea.com/servers/wls510/dtd/weblogic-ejb-jar.dtd'>

<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>School</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
        </caching-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>datasource_name</res-ref-name>
                <jndi-name>HS_OLTP</jndi-name>
            </resource-description>
            <resource-description>
                <res-ref-name>jts_datasource_name</res-ref-name>
                <jndi-name>JTS_HS_OLTP</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>com.topcoder.web.ejb.school.SchoolHome</jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>

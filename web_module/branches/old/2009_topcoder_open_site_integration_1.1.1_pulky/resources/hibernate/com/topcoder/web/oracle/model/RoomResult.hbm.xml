<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.oracle.model" auto-import="false">

    <class name="RoomResult" table="oracle_oltp:room_result">
        <composite-id name="id" class="RoomResult$Identifier">
            <key-many-to-one name="room" class="Room" column="room_id"/>
            <key-many-to-one name="user" class="com.topcoder.web.common.model.User" column="user_id"/>
        </composite-id>

        <property name="advanced"/>
<!--
        <property name="attended"/>
-->
        <property name="score"/>
        <property name="placed"/>
    </class>

</hibernate-mapping>
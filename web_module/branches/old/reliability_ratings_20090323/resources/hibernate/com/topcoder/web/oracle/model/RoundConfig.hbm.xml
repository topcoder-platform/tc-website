<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.oracle.model" auto-import="false">

    <class name="RoundConfig" table="oracle_oltp:round_config">
        <composite-id name="id" class="RoundConfig$Identifier">
            <key-many-to-one name="property" class="RoundProperty" column="property_id"/>
            <key-many-to-one name="round" class="Round" column="round_id"/>
        </composite-id>

        <property name="value" column="property_value"/>

    </class>

</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.common.model.educ">

    <class name="StudentClassroom" table="student_classroom_xref">
            <key-many-to-one name="studentId" class="User" column="student_id"/>
            <key-many-to-one name="classroomId" class="Preference" column="classroom_id"/>            
        </composite-id>

        <property name="status" column="status_id" access="field"/>
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.common.model.educ">

    <class name="Professor" table="professor">
        <id name="id" column="user_id" access="field">
            <generator class="foreign">
                <param name="property">user</param>
            </generator>
        </id>
        <one-to-one name="user" class="com.topcoder.web.common.model.User" constrained="true"/>
        <one-to-one name="school" class="com.topcoder.web.common.model.School" constrained="true"/>
        <property name="statusId" column="status_id" access="field"/>

<!-- 
        <set name="classrooms" cascade="save-update" inverse="true" access="field">
            <key column="user_id" not-null="true"/>
            <one-to-many class="Classroom"/>
        </set>
 -->
 
        <set name="students" table="student_professor_xref" inverse="true" access="field">
            <key column="professor_id"/>
            <many-to-many column="student_id" foreign-key="coder_id" unique="true" class="com.topcoder.web.common.model.Coder"/>
        </set>
    </class>
</hibernate-mapping>

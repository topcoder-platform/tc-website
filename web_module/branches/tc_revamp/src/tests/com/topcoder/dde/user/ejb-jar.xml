<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
    <enterprise-beans>
        <session>
            <ejb-name>UserManagerEJB</ejb-name>
            <local>com.topcoder.dde.user.UserManagerLocal</local>
            <local-home>com.topcoder.dde.user.UserManagerLocalHome</local-home>
            <home>com.topcoder.dde.user.UserManagerRemoteHome</home>
            <remote>com.topcoder.dde.user.UserManagerRemote</remote>
            <ejb-class>com.topcoder.dde.user.UserManagerBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <ejb-local-ref>
                <ejb-ref-name>ejb/DDECompanySizeEJB</ejb-ref-name>
                <ejb-ref-type>Entity</ejb-ref-type>
                <local-home>com.topcoder.dde.persistencelayer.interfaces.LocalDDECompanySizeHome</local-home>
                <local>com.topcoder.dde.persistencelayer.interfaces.LocalDDECompanySize</local>
                <ejb-link>DDECompanySizeEJB</ejb-link>
            </ejb-local-ref>
            <ejb-local-ref>
                <ejb-ref-name>ejb/DDECountryCodesEJB</ejb-ref-name>
                <ejb-ref-type>Entity</ejb-ref-type>
                <local-home>com.topcoder.dde.persistencelayer.interfaces.LocalDDECountryCodesHome</local-home>
                <local>com.topcoder.dde.persistencelayer.interfaces.LocalDDECountryCodes</local>
                <ejb-link>DDECountryCodesEJB</ejb-link>
            </ejb-local-ref>
            <ejb-local-ref>
                <ejb-ref-name>ejb/DDEPriceTiersEJB</ejb-ref-name>
                <ejb-ref-type>Entity</ejb-ref-type>

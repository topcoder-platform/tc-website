                <local>com.topcoder.dde.persistencelayer.interfaces.LocalDDEPriceTiers</local>
                <ejb-link>DDEPriceTiersEJB</ejb-link>
            </ejb-local-ref>
            <ejb-local-ref>
                <ejb-ref-name>ejb/DDEUserMasterEJB</ejb-ref-name>
                <ejb-ref-type>Entity</ejb-ref-type>
                <local-home>com.topcoder.dde.persistencelayer.interfaces.LocalDDEUserMasterHome</local-home>
                <local>com.topcoder.dde.persistencelayer.interfaces.LocalDDEUserMaster</local>
                <ejb-link>DDEUserMasterEJB</ejb-link>
            </ejb-local-ref>
            <ejb-local-ref>
                <ejb-ref-name>ejb/DDEUserCustomerEJB</ejb-ref-name>
                <ejb-ref-type>Entity</ejb-ref-type>
                <local-home>com.topcoder.dde.persistencelayer.interfaces.LocalDDEUserCustomerHome</local-home>
                <local>com.topcoder.dde.persistencelayer.interfaces.LocalDDEUserCustomer</local>
                <ejb-link>DDEUserCustomerEJB</ejb-link>
            </ejb-local-ref>
            <ejb-local-ref>
                <ejb-ref-name>ejb/DDEUserTechnologiesEJB</ejb-ref-name>
                <ejb-ref-type>Entity</ejb-ref-type>
                <local-home>com.topcoder.dde.persistencelayer.interfaces.LocalDDEUserTechnologiesHome</local-home>
                <local>com.topcoder.dde.persistencelayer.interfaces.LocalDDEUserTechnologies</local>
                <ejb-link>DDEUserTechnologiesEJB</ejb-link>
            </ejb-local-ref>
            <ejb-local-ref>
                <ejb-ref-name>ejb/DDETechnologyTypesEJB</ejb-ref-name>
                <ejb-ref-type>Entity</ejb-ref-type>
                <local-home>com.topcoder.dde.persistencelayer.interfaces.LocalDDETechnologyTypesHome</local-home>
                <local>com.topcoder.dde.persistencelayer.interfaces.LocalDDETechnologyTypes</local>
                <ejb-link>DDETechnologyTypesEJB</ejb-link>
            </ejb-local-ref>
        </session>
        <session>
            <description>TCS JUnit Session Bean Test Runner</description>
            <ejb-name>TCS_DDEUser_EJBTestRunnerEJB</ejb-name>
            <home>com.topcoder.dde.user.test.TCSEJBTestRunnerHome</home>
            <remote>net.sourceforge.junitejb.EJBTestRunner</remote>
            <ejb-class>com.topcoder.dde.user.test.TCSEJBTestRunnerBean</ejb-class>
            <session-type>Stateful</session-type>
            <transaction-type>Bean</transaction-type>
            <ejb-local-ref>
                <ejb-ref-name>ejb/IdGenEJB</ejb-ref-name>
                <ejb-ref-type>Session</ejb-ref-type>
                <local-home>com.topcoder.util.idgenerator.bean.LocalIdGenHome</local-home>
                <local>com.topcoder.util.idgenerator.bean.LocalIdGen</local>
                <ejb-link>IdGenEJB</ejb-link>
            </ejb-local-ref>
            <ejb-local-ref>
                <ejb-ref-name>ejb/dde/UserManagerEJB</ejb-ref-name>
                <ejb-ref-type>Session</ejb-ref-type>
                <local-home>com.topcoder.dde.user.UserManagerLocalHome</local-home>
                <local>com.topcoder.dde.user.UserManagerLocal</local>
                <ejb-link>UserManagerEJB</ejb-link>
            </ejb-local-ref>
        </session>
    </enterprise-beans>
</ejb-jar>
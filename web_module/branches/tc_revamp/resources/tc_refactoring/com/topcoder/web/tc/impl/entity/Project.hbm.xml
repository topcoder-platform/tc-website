<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
	<class name="Project" table="project">
		<id name="projectId" column="project_id">
		</id>

		<property name="projectCategoryId" column="project_category_id"
			not-null="true" />
		<property name="projectStatusId" column="project_status_id"
			not-null="true" />
		<property access="field" column="tc_direct_project_id" name="tcDirectProjectId"
			type="long" />
	</class>
</hibernate-mapping>
<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
    <class name="Submission" table="submission">
        <id name="submissionId" column="submission_id">
        </id>

		<property name="uploadId" column="upload_id" not-null="true" />
		<property name="submissionStatusId" column="submission_status_id" not-null="true" />
        <property name="screeningScore" column="screening_score" not-null="true" />
		<property name="submissionTypeId" column="submission_type_id" not-null="true" />
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
	<class name="PhaseCriteria" table="phase_criteria">
		<composite-id>
			<key-property access="field" column="project_phase_id" name="projectPhaseId"
				type="long" />
			<key-property access="field" column="phase_criteria_type_id"
				name="phaseCriteriaTypeId" type="long" />
		</composite-id>
		<property access="field" column="parameter" name="parameter"
			type="java.lang.String" />
	</class>
</hibernate-mapping>

<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
	<class name="Upload" table="upload">
		<id name="uploadId" column="upload_id">
		</id>
		<property access="field" column="resource_id" name="resourceId"
			type="long" />
		<property name="projectId" column="project_id" not-null="true" />
		<property access="field" column="upload_type_id" name="uploadTypeId"
			type="long" />
	</class>
</hibernate-mapping>
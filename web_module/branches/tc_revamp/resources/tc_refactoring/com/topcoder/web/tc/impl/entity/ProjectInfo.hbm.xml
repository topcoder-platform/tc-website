<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
    <class name="ProjectInfo" table="project_info">
        <composite-id>
		    <key-property name="projectId" column="project_id"/>
			<key-property name="projectInfoTypeId" column="project_info_type_id"/>
	    </composite-id>

		<property name="value" column="value" not-null="true" />
	</class>
</hibernate-mapping>
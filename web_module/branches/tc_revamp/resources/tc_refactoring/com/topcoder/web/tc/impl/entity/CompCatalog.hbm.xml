<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
	<class name="CompCatalog" table="comp_catalog">
		<id access="field" column="component_id" name="componentId" type="long" />
		<property access="field" column="root_category_id" name="rootCategoryId"
			type="long" />
		<property access="field" column="component_name" name="componentName"
			type="java.lang.String" />
	</class>
</hibernate-mapping>

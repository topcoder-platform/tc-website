<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.common.model">

    <class name="FileType" table="file_type_lu" mutable="false">
        <id name="id" column="file_type_id" access="field"/>
        <property name="description" column="file_type_desc" access="field"/>
        <property name="mimeType" column="mime_type" access="field"/>
        <property name="sort" access="field"/>
    </class>
</hibernate-mapping>
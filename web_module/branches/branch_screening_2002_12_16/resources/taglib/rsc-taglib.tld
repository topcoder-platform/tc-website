<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<taglib>
  <tlibversion>1.0</tlibversion>
  <jspversion>1.1</jspversion>
  <shortname>Result Set Container Tags</shortname>
  <info>
  </info>

  <tag>
    <name>iterator</name>
    <tagclass>com.topcoder.web.jsp.tag.ListIteratorTag</tagclass>
    <teiclass>com.topcoder.web.jsp.tag.ResultSetRowIteratorTagInfo</teiclass>
    <bodycontent>JSP</bodycontent>
    <info>
      Tag to iterate through a ResultSetContainer
    </info>
    <attribute>
      <name>id</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>list</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>item</name>
    <tagclass>com.topcoder.web.jsp.tag.ResultSetItemTag</tagclass>
    <bodycontent>empty</bodycontent>
    <info>
      Tag to get the value of a TCResultItem
    </info>
    <attribute>
      <name>row</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>name</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>

</taglib>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.common.model">

    <class name="TimeZone" table="timezone_lu" mutable="false">
        <id name="id" column="timezone_id" access="field"/>
        <property name="description" column="timezone_desc" access="field"/>
    </class>

</hibernate-mapping>
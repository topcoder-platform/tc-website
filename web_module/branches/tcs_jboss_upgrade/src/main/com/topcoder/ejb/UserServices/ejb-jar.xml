<?xml version="1.0"?>

<!DOCTYPE ejb-jar PUBLIC
      "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
      "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
    <description>User Services Jar</description>
    <display-name>User Services Jar</display-name>
    <enterprise-beans>
        <entity>
            <description>User Services EJB</description>
            <display-name>User Services EJB</display-name>
            <ejb-name>UserServices</ejb-name>
            <home>com.topcoder.ejb.UserServices.UserServicesHome</home>
            <remote>com.topcoder.ejb.UserServices.UserServices</remote>
<!--
            <local-home>com.topcoder.ejb.ContestAdminServices.ContestAdminServicesHome</local-home>
            <local>com.topcoder.ejb.ContestAdminServices.ContestAdminServices</local>
-->
            <ejb-class>com.topcoder.ejb.UserServices.UserServicesBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Long</prim-key-class>
            <reentrant>False</reentrant>
        </entity>
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>UserServices</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>



<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
 "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

<servlet>
  <servlet-name>MainServlet</servlet-name>
  <display-name>MainServlet</display-name>
  <description>Main Controller of Topcoder Corporate</description>
  <servlet-class>com.topcoder.web.corp.controller.MainServlet</servlet-class>

  <init-param>
    <param-name>processor-1</param-name>
    <param-value>com.topcoder.web.corp.request.ContentFetcherA</param-value>
    <description>just a stub</description>
  </init-param>

  <init-param>
    <param-name>processor-2</param-name>
    <param-value>com.topcoder.web.corp.request.ContentFetcherB</param-value>
    <description>just a stub</description>
  </init-param>
</servlet>

<servlet>
  <servlet-name>MainServletStub</servlet-name>
  <display-name>MainServletStub</display-name>
  <description>Static Processor Controller Testing of Topcoder Corporate</description>
  <servlet-class>com.topcoder.web.corp.controller.MainServletStub</servlet-class>
</servlet>

<servlet-mapping>
  <servlet-name>MainServlet</servlet-name>
  <url-pattern>/</url-pattern>
</servlet-mapping>

<servlet-mapping>
  <servlet-name>MainServletStub</servlet-name>
  <url-pattern>/hs</url-pattern>
</servlet-mapping>

</web-app>

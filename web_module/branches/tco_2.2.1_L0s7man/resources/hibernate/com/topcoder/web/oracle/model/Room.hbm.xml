<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.oracle.model" auto-import="false">

    <class name="Room" table="oracle_oltp:room">
        <id name="id" column="room_id">
            <generator class="com.topcoder.web.common.model.IdGenerator">
                <param name="sequence_name">ORACLE_ROOM_SEQ</param>
                <param name="return_class">java.lang.Integer</param>
            </generator>
        </id>
        <property name="name"/>
        <many-to-one name="round" column="round_id" class="Round"/>
        <set name="results" cascade="save-update" inverse="true">
            <key column="room_id" not-null="true"/>
            <one-to-many class="RoomResult"/>
        </set>
        <set name="candidateResults" cascade="save-update" inverse="true">
            <key column="room_id" not-null="true"/>
            <one-to-many class="CandidateRoomResult"/>
        </set>

        <set name="predictions" cascade="save-update" inverse="true">
            <key column="room_id" not-null="true"/>
            <one-to-many class="Prediction"/>
        </set>

    </class>
</hibernate-mapping>
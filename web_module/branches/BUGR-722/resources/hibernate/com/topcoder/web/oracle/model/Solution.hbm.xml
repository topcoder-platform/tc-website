<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.oracle.model" auto-import="false">

    <class name="Solution" table="oracle_oltp:solution">
        <id name="id" column="candidate_id">
            <generator class="foreign">
                <param name="property">candidate</param>
            </generator>
        </id>
        <one-to-one name="candidate" class="Candidate" constrained="true"/>
        <property name="value"/>
    </class>
</hibernate-mapping>
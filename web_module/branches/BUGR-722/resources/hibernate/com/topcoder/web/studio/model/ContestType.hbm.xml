<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.studio.model">
    <class name="ContestType" table="contest_type_lu" mutable="false">
        <id name="id" column="contest_type_id"/>
        <property name="description" column="contest_type_desc"/>
        <property name="previewImageRequired" type="boolean" column="require_preview_image"/>
        <property name="previewFileRequired" type="boolean" column="require_preview_file"/>
        <property name="includeGallery" type="boolean" column="include_gallery"/>
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.oracle.model" auto-import="false">

    <class name="CandidateRoomResult" table="oracle_oltp:candidate_room_result">
        <composite-id name="id" class="CandidateRoomResult$Identifier">
            <key-many-to-one name="room" class="Room" column="room_id"/>
            <key-many-to-one name="candidate" class="Candidate" column="candidate_id"/>
        </composite-id>

        <property name="advanced"/>
        <property name="placed"/>
        <property name="correctValue" column="correct_value" />
    </class>

</hibernate-mapping>
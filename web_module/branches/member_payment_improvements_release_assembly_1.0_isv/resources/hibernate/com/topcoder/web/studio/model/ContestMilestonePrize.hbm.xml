<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.studio.model">
    <class name="ContestMilestonePrize" table="contest_milestone_prize">
        <id name="id" column="contest_milestone_prize_id" type="long" unsaved-value="-1">
            <generator class="sequence">
                <param name="sequence">CONTEST_MILESTONE_PRIZE_SEQ</param>
            </generator>
        </id>

        <many-to-one name="type" column="prize_type_id" class="PrizeType"/>
        <property name="amount" column="amount"/>
        <property name="numberOfSubmissions" column="number_of_submissions"/>
        <property name="createDate" column="create_date"/>
    </class>
</hibernate-mapping>
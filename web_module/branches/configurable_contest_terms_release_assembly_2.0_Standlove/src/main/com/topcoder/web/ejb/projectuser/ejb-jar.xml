<?xml version="1.0"?>

<!DOCTYPE ejb-jar PUBLIC
        "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
        "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
    <description>Project User Jar</description>
    <display-name>Project User Jar</display-name>
    <enterprise-beans>
		<session>
            <description>Project User EJB</description>
            <display-name>Project User EJB</display-name>
            <ejb-name>ProjectUser</ejb-name>
            <home>com.topcoder.web.ejb.projectuser.ProjectUserHome</home>
            <remote>com.topcoder.web.ejb.projectuser.ProjectUser</remote>
            <local-home>com.topcoder.web.ejb.projectuser.ProjectUserLocalHome</local-home>
            <local>com.topcoder.web.ejb.projectuser.ProjectUserLocal</local>
            <ejb-class>com.topcoder.web.ejb.projectuser.ProjectUserBean</ejb-class>
            <transaction-type>Container</transaction-type>
            <session-type>Stateless</session-type>
        </session>
    </enterprise-beans>
    <assembly-descriptor>        
		<container-transaction>
            <method>
                <ejb-name>ProjectUser</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Supports</trans-attribute>
        </container-transaction>        
    </assembly-descriptor>
</ejb-jar>




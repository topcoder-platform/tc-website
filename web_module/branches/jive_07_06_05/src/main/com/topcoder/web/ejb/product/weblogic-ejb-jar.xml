<?xml version="1.0"?>

<!DOCTYPE weblogic-ejb-jar PUBLIC
 '-//BEA Systems, Inc.//DTD WebLogic 5.1.0 EJB//EN'
 'http://www.beasys.com/j2ee/dtds/weblogic-ejb-jar.dtd'>

<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>Product</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
            <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
        </caching-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>datasource_name</res-ref-name>
                <jndi-name>CORP_DS</jndi-name>
            </resource-description>
            <resource-description>
                <res-ref-name>jts_datasource_name</res-ref-name>
                <jndi-name>JTS_CORP_DS</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>com.topcoder.web.ejb.product.ProductHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>Purchase</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
            <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
        </caching-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>datasource_name</res-ref-name>
                <jndi-name>CORP_DS</jndi-name>
            </resource-description>
            <resource-description>
                <res-ref-name>jts_datasource_name</res-ref-name>
                <jndi-name>JTS_CORP_DS</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>com.topcoder.web.ejb.product.PurchaseHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>ProductUnit</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
            <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
        </caching-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>datasource_name</res-ref-name>
                <jndi-name>CORP_DS</jndi-name>
            </resource-description>
            <resource-description>
                <res-ref-name>jts_datasource_name</res-ref-name>
                <jndi-name>JTS_CORP_DS</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>com.topcoder.web.ejb.product.ProductUnitHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>Unit</ejb-name>
        <caching-descriptor>
            <max-beans-in-free-pool>20</max-beans-in-free-pool>
            <initial-beans-in-free-pool>0</initial-beans-in-free-pool>
        </caching-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>datasource_name</res-ref-name>
                <jndi-name>CORP_DS</jndi-name>
            </resource-description>
            <resource-description>
                <res-ref-name>jts_datasource_name</res-ref-name>
                <jndi-name>JTS_CORP_DS</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>com.topcoder.web.ejb.product.UnitHome</jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>

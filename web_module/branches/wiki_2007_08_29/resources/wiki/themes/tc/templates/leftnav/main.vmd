<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>$title - #siteTitle()</title>
    #standardHeader()
    <link type="text/css" rel="stylesheet" href="/css/tcStyles.css" />
</head>
## HTML HEADER ENDS

## HTML BODY BEGINS
#onLoad()

## TOP

<table width="100%" border="0" cellpadding="0" cellspacing="0">
<tbody>
    <tr valign="top">

## LEFT NAV
        #if ($navHelper.hasNavigation($sitemeshPage.getProperty("page.spacekey")) || $sitemeshPage.getProperty("page.pageops") || $sitemeshPage.getProperty("page.navops"))
        <td width="180">
        $navHelper.renderNavigation($sitemeshPage.getProperty("page.spacekey"))
        </td>
        #end
        <td width="100%" align="left" class="bodyColumn">

## PAGE BODY
    #if ($navHelper.hasNavigation($sitemeshPage.getProperty("page.spacekey")) || $sitemeshPage.getProperty("page.pageops") || $sitemeshPage.getProperty("page.navops"))
    $!sitemeshPage.getProperty("page.pageops")
    $!sitemeshPage.getProperty("page.navops")
    $!sitemeshPage.getProperty("page.spaceops")
    #end
    <table  border="0" cellpadding="0" cellspacing="0" width="100%">
        ## LOCATION BAR: (BREADCRUMBS AND SEARCH BOX) BEGIN
        <tr class="topBar">
            <td align="left" width="85%">
                &nbsp;
                <span class="topBarDiv"> #breadcrumbs() </span>
            </td>
            <td align="right" valign="middle" width="1%" nowrap>
                #quickSearch()
            </td>
        </tr>
    </table>
    <table border="0" cellpadding="0" cellspacing="0" width="100%">
        ## LOCATION BAR: (BREADCRUMBS AND SEARCH BOX) END

        ## INFO BAR: (LOGO, LOCATION, AND USER/GROUP OPTIONs) BEGIN
        <tr>
            <td style="padding: 5px" colspan="2">
            <table style="padding: 0px; margin: 0px 5px; width: 100%;" cellspacing="0" cellpadding="1" border="0">
                <tr>
                    <td rowspan="2" valign="bottom" width="1%">
                    <a href="$req.contextPath/dashboard.action"><img src="#logo($sitemeshPage.getProperty("page.spacekey"))" align="absmiddle" border="0"></a>
                    </td>
                    <td valign="bottom" align="left" width="1%" nowrap>
                        <span class="logoSpaceLink">#spaceTitleLink()</span>
                    </td>
                    <td align="right" valign="top" width="98%">
                        ## history, profile, admin, logout links
                        #usernavbar()
                        #printableicon()
                        #pdficon()
                    </td>
                </tr>
                <tr>
                    <td height="22" align="left" colspan="2">
                        <span class="pagetitle" style="padding: 0px; margin: 0px; text-decoration: none">
                            #pageTitleLink()
                        </span>
                    </td>
                </tr>
            </table>
            </td>
        </tr>
        ## INFO BAR: (LOGO, LOCATION, AND USER/GROUP OPTIONs) END
    </table>


        </td>
    </tr>
</tbody>
</table>

## FOOT
#parse ("/decorators/includes/footer.vmd")

</body>
</html>
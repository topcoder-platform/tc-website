<style type="text/css">

		div.popUp {
			display: none;
			position: absolute;
			top: 0px;
			left: 0px;
			margin: 0;
			z-index: 100;
		}
		div.popUp div {
			float: left;
			line-height: normal;
			font-weight: normal;
			font-size: 11px;
			background: #FFFFCC;
			border: 1px solid #999999;
			padding: 4px 6px 4px 6px;
			margin: 0;
			white-space: nowrap;
		}
		div.popUp li {
			list-style-type: disc;
			margin-left: 16px;
		}
		span.bodySubtitle {
			font-size: 14px;
			font-weight: bold;
			background: none;
			color: black;
			padding-left: 0px;
			padding-bottom: 0px;
			margin-left: 0px;
		}
		.bigRed{
			color: #FF0000; 
			font-size: 12px; 
			font-weight: bold; 
			text-decoration: none;
			margin: 10px;
		}
		span.required {
			background: none;
			color: black;
		}
		.exp {
			color: #FF0000; 
			font-size: 12px; 
			font-weight: bold; 
			text-decoration: none;
			margin: 0px;
			padding: 0px;
		}
		span.schoolName {
			background: none;
			margin: 0px;
			padding: 0px;
			color: black;				
		}
		.small { 
			font-size: 10px;
			margin-bottom: 4px;
			margin-right: 2px;
		}
</style>

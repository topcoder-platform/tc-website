<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.topcoder.web.reg.model.AlgoRating" table="algo_rating">
        <property name="coderId" column="coder_id"/>
        <property name="ratingTypeId" column="algo_rating_type_id"/>
        <property name="rating"/>
        <property name="roundId" column="round_id"/>
        <property name="numRatings" column="num_ratings"/>
    </class>
</hibernate-mapping>
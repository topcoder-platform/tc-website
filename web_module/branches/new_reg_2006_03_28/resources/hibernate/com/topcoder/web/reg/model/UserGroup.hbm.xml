<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.reg.model">

    <class name="UserGroup" table="user_group_xref" mutable="false">
        <id name="id" column="user_group_id" access="field"/>
        <property name="userId" column="login_id" access="field"/>
        <property name="groupId" column="group_id" access="field"/>
        <property name="createUserId" column="create_user_id" access="field"/>
        <property name="securityStatusid" column="security_status_id" access="field"/>
    </class>

</hibernate-mapping>


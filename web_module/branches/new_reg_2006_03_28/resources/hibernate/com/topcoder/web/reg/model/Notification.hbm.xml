<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.topcoder.web.reg.model.Notification" table="notify_lu">
        <id name="id" column="notify_id"/>
        <property name="name"/>
        <property name="status"/>
        <property name="sort"/>
        <set name="registrationTypes" table="registration_type_notify_xref" cascade="none" outer-join="false">
            <key column="notify_id"/>
            <many-to-many column="registration_type_id" class="com.topcoder.web.reg.model.RegistrationType"/>
        </set>
    </class>

</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.topcoder.web.reg.model.School" table="school" mutable="false">
        <id name="id" column="school_id">
            <generator class="com.topcoder.web.reg.model.IdGenerator">
                <param name="sequence_name">SCHOOL_SEQ</param>
            </generator>
        </id>
        <many-to-one name="address" column="address_id" class="com.topcoder.web.reg.model.Address"/>
        <property name="userId" column="userId"/>
        <property name="name"/>
        <property name="shortName"/>
        <many-to-one name="type" column="school_type_id" class="com.topcoder.web.reg.model.SchoolType"/>
    </class>
</hibernate-mapping>
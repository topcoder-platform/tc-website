<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.topcoder.web.reg.model.Coder" table="coder">
        <property name="memberSince" type="timestamp" column="member_since"/>
        <property name="quote"/>
        <property name="coderTypeId" column="coder_type_id"/>
        <property name="compCountryCode" column="comp-country_code"/>

        <set name="algoRatings" cascade="save-update">
            <key column="coder_id"/>
            <one-to-many class="com.topcoder.web.reg.model.AlgoRating"/>
        </set>
    </class>

</hibernate-mapping>
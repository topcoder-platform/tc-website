        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.topcoder.web.reg.model.State" table="state">
        <id name="code" column="state_code"/>
        <property name="name" column="state_name"/>
    </class>

</hibernate-mapping>
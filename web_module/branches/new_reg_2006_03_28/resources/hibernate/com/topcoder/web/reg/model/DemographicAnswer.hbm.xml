<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.topcoder.web.reg.model.DemographicAnswer" table="demographic_answer">
        <key name="id" column="demographic_answer_id"/>
        <property name="questionId" column="demographic_question_id"/>
        <property name="text"/>
        <property name="sort"/>
        <property name="status"/>
    </class>

</hibernate-mapping>
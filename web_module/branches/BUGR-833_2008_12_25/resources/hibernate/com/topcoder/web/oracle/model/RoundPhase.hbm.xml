<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.oracle.model" auto-import="false">

    <class name="RoundPhase" table="oracle_oltp:round_phase">
        <composite-id name="id" class="RoundPhase$Identifier">
            <key-many-to-one name="phase" class="Phase" column="phase_id"/>
            <key-many-to-one name="round" class="Round" column="round_id"/>
        </composite-id>

        <property name="startTime" column="start_time"/>
        <property name="endTime" column="end_time"/>
    </class>

</hibernate-mapping>
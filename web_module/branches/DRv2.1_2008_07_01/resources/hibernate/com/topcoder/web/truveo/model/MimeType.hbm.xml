<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.truveo.model">

    <class name="MimeType" table="mime_type_lu">
        <id name="id" column="mime_type_id" />
        <property column="mime_type_desc" name="description" />
        <many-to-one name="fileType" column="file_type_id"  class="TruveoFileType"/>
    </class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.web.common.model.algo">

  <class name="Language" table="language" mutable="false">
        <id name="id" column="language_id" access="field"/>
        <property name="name" column="language_name" access="field"/>
    </class>
  
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.csf.model">

    <class name="FilePath" table="path">
        <id name="id" column="path_id">
            <generator class="com.topcoder.web.common.model.IdGenerator">
                <param name="sequence_name">CSF_PATH_SEQ</param>
            </generator>
        </id>
        <property name="path" access="field"/>
    </class>
</hibernate-mapping>
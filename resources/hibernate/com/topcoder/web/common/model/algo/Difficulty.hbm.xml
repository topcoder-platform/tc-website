<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.topcoder.web.common.model.algo">

    <class name="Difficulty" table="difficulty" mutable="false">
        <id name="id" column="difficulty_id" access="field"/>
        <property name="difficultyLevel" column="difficulty_level" access="field"/>
        <property name="difficultyDesc" column="difficulty_desc" access="field"/>
        <property name="pointValue" column="point_value" access="field"/>
    </class>
</hibernate-mapping>